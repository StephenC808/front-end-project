---
import Banner from '../../components/Banner.astro';
import CardContainer from '../../components/CardContainer.astro';
import Card4Col from '../../components/Card4Col.astro';
import SiteLayout from '../../layouts/SiteLayout.astro';
import ToDoListAstro from '../../components/ToDoListAstro.astro';
import UserCard from '../../components/UserCard.svelte';
---

<SiteLayout title="Astro, Swelte, Bootstrap Demo">
	<Banner imageURL="/images/banner.jpg" />
	<div class="container pt-3">
		<h1>Ulster</h1>
	</div>
	<CardContainer>
		<UserCard client:load county="Donegal"/>
		<UserCard client:load county="Derry"/>
		<UserCard client:load county="Antrim"/>
		<UserCard client:load county="Down UK"/>
		<UserCard client:load county="Tyrone UK"/>
		<UserCard client:load county="Armagh"/>
		<UserCard client:load county="Fermanagh"/>
		<UserCard client:load county="Cavan"/>
		<UserCard client:load county="Monaghan"/>
	</CardContainer>
</SiteLayout>

